<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    <!--
    OWASP Dependency Check Suppression File
    
    This file contains suppressions for false positives identified during dependency scanning.
    Each suppression should include:
    - Clear justification for why it's a false positive
    - CVE ID or vulnerability pattern
    - Affected file/dependency pattern
    - Review date for periodic re-evaluation
    
    Format: https://jeremylong.github.io/DependencyCheck/general/suppression.html
    -->
    
    <!-- Example suppression (remove when actual suppressions are needed):
    <suppress>
        <notes><![CDATA[
        False positive: This vulnerability affects a different component than what we use.
        Review date: 2024-12-31
        ]]></notes>
        <packageUrl regex="true">^pkg:pypi/example-package@.*$</packageUrl>
        <cve>CVE-2023-12345</cve>
    </suppress>
    -->
    
    <!-- Development dependencies that don't affect production -->
    <suppress>
        <notes><![CDATA[
        Development/testing dependencies are not deployed to production.
        These vulnerabilities do not affect the runtime security of the application.
        Review date: 2025-06-01
        ]]></notes>
        <packageUrl regex="true">^pkg:pypi/(pytest|bandit|black|flake8|mypy)@.*$</packageUrl>
    </suppress>
    
    <!-- Suppress vulnerabilities in test files -->
    <suppress>
        <notes><![CDATA[
        Test files are not deployed to production and do not affect runtime security.
        Review date: 2025-06-01
        ]]></notes>
        <filePath regex="true">.*/(test|tests)/.*</filePath>
    </suppress>
    
    <!-- Suppress vulnerabilities in documentation -->
    <suppress>
        <notes><![CDATA[
        Documentation files are not part of the runtime application.
        Review date: 2025-06-01
        ]]></notes>
        <filePath regex="true">.*/docs/.*</filePath>
    </suppress>
    
    <!-- Suppress vulnerabilities in development scripts -->
    <suppress>
        <notes><![CDATA[
        Development scripts are not deployed to production.
        Review date: 2025-06-01
        ]]></notes>
        <filePath regex="true">.*/scripts/.*</filePath>
    </suppress>
</suppressions>
